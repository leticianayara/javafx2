<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ViewController">
   <children>
      <Button fx:id="btSum" layoutX="35.0" layoutY="180.0" mnemonicParsing="false" onAction="#onBtSumAction" text="Sum" />
      <Label layoutX="35.0" layoutY="38.0" text="Calculate Sum">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="txtNumber1" layoutX="35.0" layoutY="67.0" promptText="First number" />
      <TextField fx:id="txtNumber2" layoutX="35.0" layoutY="138.0" promptText="Second number" />
      <Label layoutX="35.0" layoutY="237.0" text="Result =" />
      <Label fx:id="labelResult" layoutX="82.0" layoutY="237.0" />
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</AnchorPane>
